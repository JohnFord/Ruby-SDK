<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<%= stylesheet_link_tag "main" %>
<script type="text/javascript" src="script/common.js"></script>
</head>
<body>
<div id="container">
     <%=render :partial => "header/header" , :locals => { :selectedHeader => "transaction" ,:selectedSubMenu => "transactionValidExpDate"} %>
	<div id="content">
		<h1>Valid Card Exp Date</h1>
		<%= form_for :post , url: { action: :transactvalidexpdaterequest } do |f| %>
			<table cellpadding="5" cellspacing="5">
				<tr>	
					<td colspan="2">
						&nbsp;
					</td>						
				</tr>
				<tr>
					<td style="width: 50%;">
						URL 
					</td>
					<td>
						<select name="URL_MODE" class="combo_width">
							<option value="uat">UAT</option>
							<option value="production">Production</option>
					       </select>
					</td>
				</tr>
				  <tr>	
					<td>
						Exp Date
					</td>
					<td>
						<input type="text" name="expDate"/>
					</td>			
				</tr>								
				
				<tr>	
					<td colspan="2">&nbsp;</td>
				</tr>
				<tr>	
					<td colspan="2">
						<input type="submit" value="OK" class="submit"/>					
					</td>						
				</tr>
			</table>
		<% end %>
		<%=render :partial => "webserviceresponse/webserviceresponse", :locals => { :resp => @resp } %>
	  </div>
	<!-- content div ends here -->
	<div class="clearfix"></div>
</div>
<!-- container div ends here -->
<%=render :partial => "footer/footer" %>
</body>
</html>
