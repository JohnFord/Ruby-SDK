
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<%= stylesheet_link_tag "main" %>
<%= javascript_include_tag "jquery-1.7.2.min" %>
<%= javascript_include_tag "common"   %>
</head>
<script type="text/javascript">
$(document).ready(function() {
	var height = $("#response_tbl").height();
	if(height != null && height < 300){
		$("#response_div").height(height);
	}
});
</script>
<body>
<div id="container">
	<%=render :partial => "header/header" , :locals => { :selectedHeader => "report" ,:selectedSubMenu => "getCheckTrx"} %>
	<div id="content">
		<h1>Get Check Transactions</h1>
		<%= form_for :post , url: { action: :getchecktrxrequest } do |f| %>
			<table cellpadding="5" cellspacing="5" style="width: 100%;">
				<tr>	
					<td colspan="4" style="text-align: center;">
						<%=render :partial => "urlmerchantcombo/urlmerchantcombo" %>
					</td>											
				</tr>	
				
				<tr>	
					<td colspan="4">
						&nbsp;
					</td>											
				</tr>
				
				<tr>	
					<td>
						RPNum
					</td>
					<td>
						<input type="text" id="rpnum" name="rpnum"/>
					</td>
					<td>
						PNRef
					</td>
					<td>
						<input type="text" id="pnref" name="pnref"/>
					</td>
				</tr>								
				
				<tr>	
					<td>
						Begin Date
					</td>
					<td>
						<input type="text" id="beginDt" name="beginDt"/>
					</td>
					<td>
						End Date
					</td>
					<td>
						<input type="text" id="endDt" name="endDt"/>
					</td>			
				</tr>
				
				<tr>					
					<td>
						Payment Type
					</td>
					<td>
						<input type="text" id="paymentType" name="paymentType"/>
					</td>
					<td>
						Exclude Payment Type
					</td>
					<td>
						<input type="text" id="excludePaymentType" name="excludePaymentType" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						Trans Type
					</td>
					<td>
						<input type="text" id="transType" name="transType" />
					</td>
					<td>
						ExcludeTransType
					</td>
					<td>
						<input type="text" id="excludeTransType" name="excludeTransType" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						Approval Code
					</td>
					<td>
						<input type="text" id="approvalCode" name="approvalCode" />
					</td>
					<td>
						Result
					</td>
					<td>
						<input type="text" id="result" name="result" />
					</td>
				</tr>
				
				<tr>	
					<td>
						Exclude Result
					</td>
					<td>
						<input type="text" id="excludeResult" name="excludeResult" />
					</td>
					<td>
						Name On Check
					</td>
					<td>
						<input type="text" id="nameOnCheck" name="nameOnCheck" />
					</td>
				</tr>
				
				<tr>
					<td>
						Check Num
					</td>
					<td>
						<input type="text" id="checkNum" name="checkNum"/>
					</td>	
					<td>
						Acct Num
					</td>
					<td>
						<input type="text" id="acctNum" name="acctNum"/>
					</td>	
				</tr>
				
				<tr>	
					<td>
						Route Num
					</td>
					<td>
						<input type="text" id="routeNum" name="routeNum"/>
					</td>
					<td>
						Exclude Void
					</td>
					<td>
						<select id="excludeVoid" name="excludeVoid" class="combo_width">
							<option value="">Select</option>
							<option value="TRUE">TRUE</option>
							<option value="FALSE">FALSE.</option>
						</select>
					</td>
				</tr>
				
				<tr>
					<td>
						User
					</td>
					<td>
						<input type="text" id="user" name="user"/>
					</td>	
					<td>
						Invoice Id
					</td>
					<td>
						<input type="text" id="invoiceId" name="invoiceId"/>
					</td>
				</tr>
				
				<tr>	
					<td>
						Settle Flag
					</td>
					<td>
						<select id="settleFlag" name="settleFlag" class="combo_width">
							<option value="">Select</option>
							<option value="TRUE">TRUE</option>
							<option value="FALSE">FALSE.</option>							
						</select>
					</td>
					<td>
						Settle Msg
					</td>
					<td>
						<input type="text" id="settleMsg" name="settleMsg"/>
					</td>
				</tr>
				
				<tr>	
					<td>
						Settle Date
					</td>
					<td>
						<input type="text" id="settleDt" name="settleDt" />
					</td>
					<td>
						extData
					</td>
					<td>
						<input type="text" id="extData" name="extData"/>
					</td>
				</tr>		
				
				<tr>	
					<td colspan="4">&nbsp;</td>
				</tr>
				<tr>	
					<td colspan="4">
						<input type="submit" value="OK" class="submit"/>					
					</td>						
				</tr>					
			</table>
		<% end %>
		<%=render :partial => "getchecktrxsummaryresponse/getchecktrxsummaryresponse", :locals => { :resp => @resp } %>
	</div>
	<!-- content div ends here -->
	<div class="clearfix"></div>
</div>
<!-- container div ends here -->
<%=render :partial => "footer/footer" %>
</body>
</html>
