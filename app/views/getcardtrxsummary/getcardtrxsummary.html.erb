
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<%= stylesheet_link_tag "main" %>
<%= javascript_include_tag "jquery-1.7.2.min" %>
<%= javascript_include_tag "common"   %>
</head>
<script type="text/javascript">
$(document).ready(function() {
	var height = $("#response_tbl").height();
	if(height != null && height < 300){
		$("#response_div").height(height);
	}
});
</script>
<body>
<div id="container">
<%=render :partial => "header/header", :locals => { :selectedHeader => "report" ,:selectedSubMenu => "getCardTrxSummary"} %>
	<div id="content">
		<h1>Get Card Transactions Summary</h1>
		<%= form_for :post , url: { action: :getcardtrxsummaryrequest } do |f| %>
			<table cellpadding="5" cellspacing="5" style="width: 100%;">
				<tr>	
					<td colspan="4" style="text-align: center;">
						<%=render :partial => "urlmerchantcombo/urlmerchantcombo" %>
					</td>											
				</tr>	
				
				<tr>	
					<td colspan="4">
						&nbsp;
					</td>											
				</tr>
				
				<tr>	
					<td>
						RPNum
					</td>
					<td>
						<input type="text" id="rpnum" name="rpnum"/>
					</td>
					<td>
						Begin Date
					</td>
					<td>
						<input type="text" id="beginDt" name="beginDt"/>
					</td>
				</tr>								
				
				<tr>	
					<td>
						End Date
					</td>
					<td>
						<input type="text" id="endDt" name="endDt"/>
					</td>	
					<td>
						Approval Code
					</td>
					<td>
						<input type="text" id="approvalCode" name="approvalCode"/>
					</td>		
				</tr>
				
				<tr>					
					<td>
						register
					</td>
					<td>
						<input type="text" id="register" name="register"/>
					</td>
					<td>
						Name On Card
					</td>
					<td>
						<input type="text" id="nameOnCard" name="nameOnCard" />
					</td>			
				</tr>			
				
				<tr>
					<td>
						Card Num
					</td>
					<td>
						<input type="text" id="cardNum" name="cardNum"/>
					</td>
					<td>
						Card Type
					</td>
					<td>
						<input type="text" id="cardType" name="cardType"/>
					</td>	
				</tr>
				
				<tr>	
					<td>
						Exclude Void
					</td>
					<td>
						<select id="excludeVoid" name="excludeVoid" class="combo_width">
							<option value="">Select</option>
							<option value="TRUE">TRUE</option>
							<option value="FALSE">FALSE.</option>
						</select>
					</td>
					<td>
						User
					</td>
					<td>
						<input type="text" id="user" name="user"/>
					</td>
				</tr>
				
				<tr>	
					<td>
						Settle Flag
					</td>
					<td>
						<select id="settleFlag" name="settleFlag" class="combo_width">
							<option value="">Select</option>
							<option value="TRUE">TRUE</option>
							<option value="FALSE">FALSE.</option>							
						</select>
					</td>
					<td>
						Settle Msg
					</td>
					<td>
						<input type="text" id="settleMsg" name="settleMsg"/>
					</td>			
				</tr>
				
				<tr>	
					<td>
						Settle Date
					</td>
					<td>
						<input type="text" id="settleDt" name="settleDt" />
					</td>
					<td>
						extData
					</td>
					<td>
						<input type="text" id="extData" name="extData"/>
					</td>
				</tr>
								
				<tr>	
					<td colspan="4">&nbsp;</td>
				</tr>
				<tr>	
					<td colspan="4">
						<input type="submit" value="OK" class="submit"/>					
					</td>						
				</tr>					
			</table>
		<% end %>
		<%=render :partial => "getcardtrxsummaryresponse/getcardtrxsummaryresponse", :locals => { :resp => @resp } %>
	 </div>
	<!-- content div ends here -->
	<div class="clearfix"></div>
</div>
<!-- container div ends here -->
<%=render :partial => "footer/footer" %>
</body>
</html>
