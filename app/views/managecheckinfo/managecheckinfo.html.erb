					<td>
						Country ID
					</td>
					<td>
						<input type="text" id="countryID" name="countryID"/>
					</td>			
				</tr>
				
				<tr>					
					<td>
						Ext Data
					</td>
					<td>
						<input type="text" id="extData" name="extData"/>
					</td>	
					<td colspan="2">&nbsp;</td>		
				</tr>		
				
				<tr>	
					<td colspan="4">&nbsp;</td>
				</tr>
				<tr>	
					<td colspan="4">
						<input type="submit" value="OK" class="submit"/>					
					</td>						
				</tr>
			</table>
		<% end %>
		<%=render :partial => "webserviceresponse/webserviceresponse", :locals => { :resp => @resp } %>
	 </div>
	<!-- content div ends here -->
	<div class="clearfix"></div>
</div>
<!-- container div ends here -->
<%=render :partial => "footer/footer" %>
