					<td colspan="4" style="text-align: center;">
						<%=render :partial => "urlmerchantcombo/urlmerchantcombo" %>
					</td>											
				</tr>	
				
				<tr>	
					<td colspan="4">
						&nbsp;
					</td>											
				</tr>			
								
				<tr>	
					<td>
						Transaction Type
					</td>
					<td>
						<select id="transType" name="transType" class="combo_width">
							<option value="Add">Add</option>
							<option value="Update">Update</option>
							<option value="Delete">Delete</option>							
						</select>
					</td>
					<td>
						Vendor
					</td>
					<td>
						<input type="text" id="vendor" name="vendor"/>
					</td>
				</tr>				
				
				<tr>	
					<td>
						Customer Key
					</td>
					<td>
						<input type="text" id="customerKey" name="customerKey"/>
					</td>
					<td>
						Check Info Key
					</td>
					<td>
						<input type="text" id="checkInfoKey" name="checkInfoKey"/>
					</td>			
				</tr>	
				
				<tr>
					<td>
						Check Type
					</td>
					<td>
						<select id="checkType" name="checkType" class="combo_width">
							<option value="Personal">Personal</option>
							<option value="Business">Business</option>							
						</select>
					</td>
					
					<td>
						Account Type
					</td>
					<td>
						<select id="accountType" name="accountType" class="combo_width">
							<option value="Checking">Checking</option>
							<option value="Savings">Savings</option>							
						</select>
					</td>
				</tr>	
				
				<tr>	
					<td>
						Check Num
					</td>
					<td>
						<input type="text" id="checkNum" name="checkNum" />
					</td>
					<td>
						MICR
					</td>
					<td>
						<input type="text" id="micr" name="micr" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						Account Num
					</td>
					<td>
						<input type="text" id="accountNum" name="accountNum" />
					</td>
					<td>
						Transit Num
					</td>
					<td>
						<input type="text" id="transitNum" name="transitNum" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						SSN
					</td>
					<td>
						<input type="text" id="ss" name="ss" />
					</td>
					<td>
						DOB
					</td>
					<td>
						<input type="text" id="dob" name="dob" />
					</td>			
				</tr>
				
				<tr>	
