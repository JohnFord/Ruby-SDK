<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<link rel="stylesheet" type="text/css" href="../../assets/stylesheets/main.css"/>
<script type="text/javascript" src="script/common.js"></script>
<%= stylesheet_link_tag "main"   %>
</head>
<body>
<div id="container">
  <%=render :partial => "header/header", :locals => { :selectedHeader => "transaction" ,:selectedSubMenu => "transactionCheck"} %>
   <div id="content">
		<h1>Process Check</h1>
		<%= form_for :post , url: { action: :transactprocesscheckrequest } do |f| %>
			<table cellpadding="5" cellspacing="5">
				<tr>	
					<td colspan="2">
						&nbsp;
					</td>						
				</tr>
				<tr>
					<td style="width: 50%;">
						URL
					</td>
					<td>
						<select name="URL_MODE" class="combo_width">
							<option value="uat">UAT</option>
							<option value="production">Production</option>
					       </select>
					</td>
				</tr>
				<tr>
					<td style="width: 50%;">
						Transaction Type
					</td>
					<td>
						<select name="transType" class="combo_width">
							<option value="Sale">Sale</option>
							<option value="Auth">Auth</option>
							<option value="Return">Return</option>
							<option value="Void">Void</option>
							<option value="Force">Force</option>
						</select>
					</td>
				</tr>
				<tr>	
					<td>
						Check Num
					</td>
					<td>
						<input type="text" name="checkNum"/>
					</td>			
				</tr>
				
				<tr>	
					<td>
						Transit Num
					</td>
					<td>
						<input type="text" name="transitNum"/>
					</td>			
				</tr>
				
				<tr>	
					<td>
						Account Num
					</td>
					<td>
						<input type="text" name="accountNum"/>
					</td>			
				</tr>		
				
				<tr>	
					<td>
						Amount
					</td>
					<td>
						<input type="text" name="total" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						MICR
					</td>
					<td>
						<input type="text" name="micr" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						Name On Check
					</td>
					<td>
						<input type="text" name="nameOnCheck"/>
					</td>			
				</tr>
				
				<tr>	
					<td>
						DL
					</td>
					<td>
						<input type="text" name="dl"/>
					</td>			
				</tr>
				
				<tr>	
					<td>
						SSN
					</td>
					<td>
						<input type="text" name="ss"/>
					</td>			
				</tr>
				
				<tr>	
					<td>
						DOB
					</td>
					<td>
						<input type="text" name="dob"/>
					</td>			
				</tr>
				
				<tr>	
					<td>
						State Code
					</td>
					<td>
						<input type="text" name="stateCode" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						Check Type
					</td>
					<td>
						<input type="text" name="checkType"/>
					</td>			
				</tr>	
				
				<tr>	
					<td>
						Ext Data
					</td>
					<td>
						<input type="text" name="extData"/>
					</td>			
				</tr>			
				
				<tr>	
					<td colspan="2">&nbsp;</td>
				</tr>
				<tr>	
					<td colspan="2">
						<input value="OK" type="submit" class="submit"/>					
					</td>						
				</tr>
			</table>
		
		<% end %>
		<%=render :partial => "webserviceresponse/webserviceresponse", :locals => { :resp => @resp } %>
	</div>
	<!-- content div ends here -->
	<div class="clearfix"></div>
</div>
<!-- container div ends here -->
<%=render :partial => "footer/footer" %>
</body>
</html>
