
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<%= stylesheet_link_tag "main" %>
<script type="text/javascript" src="script/common.js"></script>
</head>
<body>
<div id="container">
	<%=render :partial => "header/header" , :locals => { :selectedHeader => "merchant" ,:selectedSubMenu => "manageCreditInfo"}  %>
	<div id="content">
		<h1>Manage Credit Card Info</h1>
		<%= form_for :post , url: { action: :managecreditinforequest } do |f| %>
			<table cellpadding="5" cellspacing="5" style="width: 100%;">
				<tr>	
					<td colspan="4" style="text-align: center;">
						<%=render :partial => "urlmerchantcombo/urlmerchantcombo" %>
					</td>											
				</tr>	
				
				<tr>	
					<td colspan="4">
						&nbsp;
					</td>											
				</tr>			
								
				<tr>	
					<td>
						Transaction Type
					</td>
					<td>
						<select id="transType" name="transType" class="combo_width">
							<option value="Add">Add</option>
							<option value="Update">Update</option>
							<option value="Delete">Delete</option>							
						</select>
					</td>
					<td>
						Vendor
					</td>
					<td>
						<input type="text" id="vendor" name="vendor"/>
					</td>
				</tr>				
				
				<tr>	
					<td>
						Customer Key
					</td>
					<td>
						<input type="text" id="customerKey" name="customerKey"/>
					</td>
					<td>
						Check Info Key
					</td>
					<td>
						<input type="text" id="cardInfoKey" name="cardInfoKey"/>
					</td>			
				</tr>				
				
				<tr>	
					<td>
						CC Account Num
					</td>
					<td>
						<input type="text" id="ccAccountNum" name="ccAccountNum"/>
					</td>
					<td>
						CC Exp Date
					</td>
					<td>
						<input type="text" id="ccExpDate" name="ccExpDate"/>
					</td>			
				</tr>		
				
				<tr>	
					<td>
						CC Name On Card
					</td>
					<td>
						<input type="text" id="ccNameonCard" name="ccNameonCard" />
					</td>
					<td>
						CC Street
					</td>
					<td>
						<input type="text" id="ccStreet" name="ccStreet" />
					</td>			
				</tr>
				
				<tr>	
					<td>
						CC Zip
					</td>
					<td>
						<input type="text" id="ccZip" name="ccZip" />
					</td>					
					<td>
						Ext Data
					</td>
					<td>
						<input type="text" id="extData" name="extData"/>
					</td>
								
				</tr>
				
				<tr>	
					<td colspan="4">&nbsp;</td>
				</tr>
				<tr>	
					<td colspan="4">
						<input type="submit" value="OK" class="submit"/>					
					</td>						
				</tr>
			</table>
		<% end %>
		<%=render :partial => "webserviceresponse/webserviceresponse", :locals => { :resp => @resp } %>
	</div>
	<!-- content div ends here -->
	<div class="clearfix"></div>
</div>
<!-- container div ends here -->
<%=render :partial => "footer/footer" %>
</body>
</html>
